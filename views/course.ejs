<!-- start Section: Design Block for syllabus -->
<section class="text-center">
  <div class="card mx-4 mx-md-5 shadow-5-strong" style="background: hsla(0, 0%, 100%, 0.8); backdrop-filter: blur(30px); ">
      <div class="card-body py-5 px-md-5">
          <div class="row d-flex justify-content-center">
              <div class="col-lg-12">
                  <h2 class="fw-bold mb-3 text-2xl text-blue-700">SYLLABUS</h2>
                  
                  <!-- ---------------HEADER---------------- -->
                  <div class="row text-white bg-gray-700" style="text-align: center; border-bottom: 1px solid lightgray;">
                    <div class="form-group col-md-2 mb-1">
                      PDF ID                   
                    </div>
                    <div class="form-group col-md-7 mb-1">
                      PDF NAME
                    </div>
                    <div class="form-group col-md-3 mb-1">
                      UPLOADED / LAST UPDATED
                    </div>
                  </div>

                  <!-- pdf rendering for syllabus -->
                  <% pdfs1.forEach(function(pdf){ %>
                    <div class="row mb-2" style="text-align: center; border-bottom: 1px solid rgb(181, 180, 180);">
                      <!-- for pdf id -->
                      <div class="form-group col-md-2 mb-0 flex bg-gray-300">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="2.5" stroke="green" class="w-6 h-6">
                          <path stroke-linecap="round" stroke-linejoin="round" d="M9 12.75L11.25 15 15 9.75M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                        </svg> <pre>  # <%= pdf.pdf_id %></pre>                    
                      </div>

                      <!-- for pdf url / link -->
                      <div class="form-group col-md-7 mb-0 bg-blue-300">
                        <!-- <a href="<%= pdf.pdf_link %>" target="_blank" class="text-blue-800" hidden><%= pdf.pdf_name %></a> -->
                        <form action="/ViewPdf" method="post">
                          <input type="text" class="form-control" id="pdf_link" name="pdf_link" value="<%= pdf.pdf_link %>" required readonly hidden/>
                          <button type="submit"><%= pdf.pdf_name %></button>
                        </form>
                      </div>
                      <!-- for uploaded date and time -->
                      <div class="form-group col-md-3 mb-0 bg-yellow-300">
                        <%= pdf.DateTime %>
                      </div>
                    </div>
                  <% }); %>
              </div>
          </div>
      </div>
  </div>
</section>
<!-- end Section: Design Block for syllabus -->

<div style="border-bottom: 1px solid black;"></div>





