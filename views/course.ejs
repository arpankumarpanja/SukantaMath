  <script>
    function Menu(e){
      let list = document.querySelector('ul');
      e.name === 'menu' ? (e.name = "close",list.classList.add('top-[57px]') , list.classList.add('opacity-90')) :( e.name = "menu" ,list.classList.remove('top-[57px]'),list.classList.remove('opacity-90'))
    }
  </script>






